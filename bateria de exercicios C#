                Console.WriteLine("6 – Calcula uma série numérica");
                Console.WriteLine("7 – Volume de uma esfera");
                Console.WriteLine("8 – Imprime série de 1 a 10");
                Console.WriteLine("9 – Calcula média de um aluno");
                Console.WriteLine("10 – Calcula uma expressão");
                int escolha1 = Convert.ToInt32(Console.ReadLine());
                switch (escolha1)
                {
                    case 1:
                        Somatoria();
                        break;

                    case 2:
                        Reajuste();
                        break;

                    case 3:
                        Perfeito();
                        break;

                    case 4:
                        DezNotas();
                        break;

