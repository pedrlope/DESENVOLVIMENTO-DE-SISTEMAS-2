//EX2
static void EX2()
{
    Console.WriteLine("Digite 1 para media harmonica, 2 para media aritimedica e 3 para media geometrica");
    int mediaescolha = Convert.ToInt32(Console.ReadLine);
    switch (mediaescolha)
    {

        case 1:
            Console.WriteLine("qual a primeira nota");
            int nota1 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a segunda nota");
            int nota2 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a terceira nota");
            int nota3 = Convert.ToInt32(Console.ReadLine());

            int n = 3;

            int formula = n / ((1 / nota1) + (1 / nota2) + (1 / nota3));
            Console.WriteLine($" a media aritimedica é {formula}");
            break;

        case 2:
            Console.WriteLine("qual a primeira nota");
            int nota4 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a segunda nota");
            int nota5 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a terceira nota");
            int nota6 = Convert.ToInt32(Console.ReadLine());
            
            int formula2 = (nota4 + nota5  + nota6 ) /3;
            Console.WriteLine($" a media ponderada é {formula2}");
            break;

            case 3:
            Console.WriteLine("qual a primeira nota");
            int nota7 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a segunda nota");
            int nota8 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a terceira nota");
            int nota9 = Convert.ToInt32(Console.ReadLine());

            double formula3 = Math.Pow(nota7 * nota8 * nota9, 1.0 / 3.0);
            Console.WriteLine($" a media ponderada é {formula3}");
            break;
    }
}
