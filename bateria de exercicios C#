            if (!(numero <= 5))
            {
                Console.WriteLine("O número é maior que 5.");

            }
            else
            {
                Console.WriteLine("O número é menor ou igual a 5.");
            }
        }

        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
