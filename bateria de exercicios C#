static void exercicio1()
{
    int i = 1;
    while (i > 0)
    {
        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - volume de uma caixa");
        Console.WriteLine("2 - mudança de celsius para farenheit e vice e versa");
        Console.WriteLine("3 - volume de um cilindro");
        Console.WriteLine("4 - criação de coelhos");
        Console.WriteLine("5 - consumo de combustivel");
        Console.WriteLine("6 - diferença quadratica");
        Console.WriteLine("7 - propriedade distribuitiva");
        Console.WriteLine("8 - volume de uma esfera");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                caixa();
                break;

            case 2:
                temperatura();
                break;

            case 3:
                cilindro();
                break;

            case 4:
                coelhos();
                break;

            case 5:
                combustivel();
                break;

            case 6:
                diferença();
                break;

            case 7:
                distribuitiva();
                break;

            case 8:
                esfera();
                break;
        }

        //caixa
        static void caixa()
        {
            int altura, largura, profundidade;

            Console.WriteLine("qual a altura em metros da caixa");
            altura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a largura em metros da caixa");
            largura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a profundidade em metros da caixa");
            profundidade = Convert.ToInt32(Console.ReadLine());
            int formula;
            formula = largura * altura * profundidade;
            Console.WriteLine($" o volume da caixa é {formula} ");
        }
        //caixa


        //temperatura
        static void temperatura()
        {
            float temperatura, c, f;
            char unidade;

            Console.WriteLine("qual a temperatura?");
            temperatura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a unidade de medida?");
            unidade = Char.ToUpper(Convert.ToChar(Console.ReadLine()));
            f = (9 * temperatura) / 5 + 32;
            c = (5 * (temperatura - 32)) / 9;
            if (unidade == 'C' || unidade == 'c')
            {
                Console.WriteLine($" a temperatura é {f}");
            }

            if (unidade == 'F' || unidade == 'f')
            {
                Console.WriteLine($" a temperatura é {c}");
            }

        }
        //temperatura

        //cilindro
        static void cilindro()
        {
            double formula, altura, raio;

            Console.WriteLine("qual o raio do cilindro:");
            raio = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("qual a altura do cilindro:");
            altura = Convert.ToDouble(Console.ReadLine());

            formula = Math.PI * Math.Pow(raio, 2) * altura;

            Console.WriteLine($"o volume é {formula}");
        }
        //cilindro

        //coelhos
        static void coelhos()
        {

        }
        //coelhos

        //consumo
        static void combustivel()
        {
            int distancia, litros;

            Console.WriteLine("qual a distancia percorrida?");
            distancia = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("em quanto litros?");
            litros = Convert.ToInt32(Console.ReadLine());
            int formula = distancia /  litros;
            Console.WriteLine($"o consumo e de:{formula}");


        }
        //consumo

        //diferença expoenecial
        static void diferença()
        {

        }
        //diferença expoenecial

        //
        static void distribuitiva()
        {

        }
        //

        //volume de esfera
        static void esfera()
        {
            Console.WriteLine("qual o raio do cilindro:");
           double r = Convert.ToDouble(Console.ReadLine());
            double formula;
            formula = Math.PI * Math.Pow(r, 3)/4;
            Console.WriteLine($"o volume da esfera é {formula}");
        }
        //volume de esfera
        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha = Convert.ToInt32(Console.ReadLine());

        if (escolha == 2)
        {
            i = 0;

        }
        else
        {

        }
    }
}
