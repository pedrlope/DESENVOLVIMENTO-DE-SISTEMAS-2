static void exercicio11()
{
    int i = 1;
    while (i > 0)
    {

        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1");
        Console.WriteLine("2");
        Console.WriteLine("3");
        Console.WriteLine("4");
        Console.WriteLine("5");
        Console.WriteLine("6");
        Console.WriteLine("7");
        Console.WriteLine("8");
        Console.WriteLine("9");


        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha = Convert.ToInt32(Console.ReadLine());

        if (escolha == 2)
        {
            i = 0;
        }
        else
        {

