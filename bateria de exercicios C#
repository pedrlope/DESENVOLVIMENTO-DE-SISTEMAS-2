            double resultado1 = 0;
            double resultado2 = 0;



            for (int i = 1; i <= t; i++)

            {
                double Juros = c * indice * i;
                double Montante = Juros + c;

                Console.WriteLine($"Mês {i}");
                Console.WriteLine($"O Juros é = {Juros}");
                Console.WriteLine($"O Montante é = {Montante}");
                resultado1 = Juros;
                resultado2 = Montante;
            }


        }

        static void EX7()
        {
            for (int i = 1; i <= 10; i++)
            {
                Console.WriteLine($"Funcionário {i}:");

                Console.Write("Informe o salário atual: R$ ");
                double salario = Convert.ToDouble(Console.ReadLine());

                Console.Write("Informe o percentual de reajuste (%): ");
                double percentual = Convert.ToDouble(Console.ReadLine());

                double reajuste = salario * (percentual / 100);
                double novoSalario = salario + reajuste;

                Console.WriteLine($"Reajuste: R$ {reajuste}");
                Console.WriteLine($"Novo salário: R$ {novoSalario}");
            }
        }

