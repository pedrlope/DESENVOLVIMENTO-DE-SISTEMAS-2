            {
                Console.WriteLine($"o imposto de renda é nulo");
            }

            if (salario > 2259.2)
            {
                if (salario <= 2826.65)
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.075}");
                }
                if (salario > 2826.65 && salario <= 3751.05)
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.15}");
                }

                else if (salario <= 4664.68)
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.225}");
                }

                else
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.275}");
                }
            }

        }

