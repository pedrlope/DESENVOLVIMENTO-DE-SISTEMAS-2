                for (int i = 1; i <= 10; i++)
                {
                    Console.WriteLine($"{i} X {numero} = {i * numero}");
                }
            }
            //Tabuada

            //LerNumero
            static void EX2()
            {
                bool car = true;
                while (car == true)
                {
                    Console.WriteLine("qual o número?");
                    int numero = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("escreva sim para parar?");
                    string escolha = Convert.ToString(Console.ReadLine());
                    if (escolha == "sim")
                    {
                        car = false;
                    }
                }

            }

            static void EX3()
            {
                int i = 1;
                while (i <= 20)
                {
                    int quadrado = i * i;
                    Console.WriteLine($"O quadrado de {i} é {quadrado}");
                    i++;
                }
            }

            static void EX4()
            {
                Console.WriteLine("digite a quantidade de pessoas que vai fazer a pesquisa");
                int pesquisa = Convert.ToInt32(Console.ReadLine());
                double contador = 0;
                for (int i = 1; i <= pesquisa; i++)
                {


                    Console.WriteLine("digite a sua idade");
                    int idade = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("digite a sua cor de cabelo");
                    string cabelo = (Console.ReadLine());
                    Console.WriteLine("digite a sua cor do seu olho");
                    string olhos = Convert.ToString(Console.ReadLine());


                    if (idade >= 18 && idade <= 35)
                    {
                        if (cabelo.ToLower() == "loiros")
                        {
                            if (olhos.ToLower() == "verde")
                            {
                                contador++;
                            }
                        }
                    }
                }
                double porcentagem = (contador / pesquisa) * 100;
                Console.WriteLine($"A porcentagem de mulheres de 18 a 35 anos loiras de olhos verdes é de:{porcentagem}");
            }
            static void EX5()
            {
                int contador = 0;
                int i = 1;

                while (i <= 5)
                {
                    Console.WriteLine($"Digite o {i}º número:");
                    int numero = Convert.ToInt32(Console.ReadLine());

                    if (numero < 0)
                    {
                        contador++;
                    }

                    i++;
                }

                Console.WriteLine($"Quantidade de valores negativos: {contador}");
            }


            //LerNumero

            //GeraNumeros
            static void EX6()
            {
                for (float i = 10; i <= 100; i++)
                {
                    if (i % 11 == 5)
                    {
                        Console.WriteLine($"numero divisivel por 11 com resto 5: {i}");
                    }
                }

            }
            //GeraNumeros

           

            static void EX7()
            {
                Console.WriteLine("Digite o primeiro termo da PA:");
                int a1 = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("Digite a razão da PA:");
                int r = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("Digite o número de termos:");
                int n = Convert.ToInt32(Console.ReadLine());

                for (int i = 1; i <= n; i++)
                {
                    int termo = a1 + (i - 1) * r;
                    Console.WriteLine($"Termo {i} = {termo}");
                }
             }

        //MediaTurma
        static void EX8()
            {
                Console.WriteLine("Qual o numero de alunos?");
                int alunos = Convert.ToInt32(Console.ReadLine());
                double somaDasMedias = 0;

                for (int i = 1; i <= alunos; i++)
                {
                    Console.WriteLine($"Qual a media do aluno {i}");
                    int mediaAluno = Convert.ToInt32(Console.ReadLine());
                    somaDasMedias += mediaAluno;
                }
                double formula = somaDasMedias / alunos;
                Console.WriteLine($"A media da clase = {formula}");


            }
