        static void EX9()
        {
            double totalBonus = 0;

            for (int i = 1; i <= 10; i++)
            {
                Console.WriteLine($"\nFuncionário {i}:");

                Console.Write("Informe o salário: R$ ");
                double salario = Convert.ToDouble(Console.ReadLine());

                double bonus = 0;


                if (salario <= 1000)
                {
                    bonus = salario * 0.20;
                }
                else if (salario <= 2000)
                {
                    bonus = salario * 0.15;
                }
                else if (salario <= 3000)
                {
                    bonus = salario * 0.10;
                }
                else
                {
                    bonus = salario * 0.05;
                }

                totalBonus += bonus;

                Console.WriteLine($"Bônus recebido: R$ {bonus}");
            }

            Console.WriteLine($"\nTotal geral de bônus pago: R$ {totalBonus}");
        }

        //EX10
