        static void EX5()

        {
            int l = 0;
            double dois, um, soma;
            soma = 0;
            do
            {

                for (int i = 1; i <= 10; i++)
                {
                    dois = (i * i);
                    um = i;
                    double fração = um / dois;
                    if (um % 2 != 0)
                    {
                        fração = -fração;
                    }
                    soma += fração;
                    Console.WriteLine($"O valor {i} é {um}/ {dois}");

                }
                Console.WriteLine($"a soma dos valores é: {soma}");
                Console.WriteLine($"digite 1 caso deseje parar");
                int escolha = Convert.ToInt32(Console.ReadLine());
                if (escolha == 1)
                {
                    l = 1;
                }
                else
                {

                }
            }
            while (l == 0);
        }

        //SerieNumerica
        static void EX6()
        {
            Console.WriteLine("Digite o valor de X:");
            double x = Convert.ToDouble(Console.ReadLine());

            int expoente = 25;
            double soma = 0;
            int i = 1;
            do
            {
                double termo = Math.Pow(x, expoente) / i;
                if (i % 2 == 0)
                {
                    termo = -termo;
                }
                soma += termo;
                expoente--;
                i++;
            } while (i <= 25);

            Console.WriteLine($"Resultado da série: {soma}");
        }
