        //area quadrado
        static void AreaQuadrado()
        {
            Console.WriteLine("qual o comprimento do primeiro lado");
            double lado1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("qual o comprimento do segundo lado");
            double lado2 = Convert.ToDouble(Console.ReadLine());
            double formula = lado1 * lado2;
            Console.WriteLine($" a area e {formula}");
        }
        //area quadrado

    //Numero Ao quadrado
        static void NumeroAoQuadrado()
        {
            Console.WriteLine("qual o numero?");
            double num = Convert.ToDouble(Console.ReadLine());
            double formula = num * num;
            Console.WriteLine($" a area e {formula}");
        }
        //Numero Ao quadrado
  
        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
