//EX3
static void EX3()
{
    bool wheli = true;
    do
    {
        Console.WriteLine("Qual a sua linguagem da programação");
        string programacao = Console.ReadLine();
        switch (programacao)
        {
            case "C":
                Console.WriteLine("Escreva uma linha no seu programa");
                String C = Console.ReadLine();
                if (C == "Printf" || C == "Console.WriteLine()" || C == "Print")
                {
                    wheli = false;
                    Console.WriteLine("Parabéns!!");
                }
                else
                {
                    Console.WriteLine("Isso não esta certo");
                }
                break;

            case "Python":
                Console.WriteLine("Escreva uma linha no seu programa");
                String Python = Console.ReadLine();
                if (Python == "print" ||  Python == "format" || Python == "f-strings")
                {
                    Console.WriteLine("Parabéns!!");
                    wheli = false;
                }

                else
                {
                    Console.WriteLine("Isso não esta certo");
                }
                break;

            case "Java":
                Console.WriteLine("Escreva uma linha no seu programa");
                String Java = Console.ReadLine();
                if (Java == "System.out.printf")
                { 
                    wheli = false;
                Console.WriteLine("Parabéns!!");
        } else
                {
                    Console.WriteLine("Isso não esta certo");
                }
                break;
        }
    }
    while (wheli == true);
}
