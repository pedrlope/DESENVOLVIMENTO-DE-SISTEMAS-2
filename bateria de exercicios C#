        //Perfeito
        static void EX3()
        {
            Console.WriteLine("Digite um número para verificar se é perfeito:");
            int numero = Convert.ToInt32(Console.ReadLine());
            int soma = 0;
            int i = 1;
            do
            {
                if (numero % i == 0)

                    soma += i;

                i++;
            } while (i < numero);

            if (soma == numero)
            {
                Console.WriteLine($"{numero} é um número perfeito!");
            }
            else
            {
                Console.WriteLine($"{numero} não é um número perfeito.");
            }
        }
        //Perfeito

