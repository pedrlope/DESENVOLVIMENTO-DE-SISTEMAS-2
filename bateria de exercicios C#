                        Esfera();
                        break;

                    case 8:
                        SerieDez();
                        break;

                    case 9:
                        MediaDeAluno();
                        break;

                    case 10:
                        ExpressãoH();
                        break;
                }

                //reajuste salarial
                static void Reajuste()
                {

                }
                //reajuste salarial

                //Perfeito
                static void Perfeito()
                {

                }
                //Perfeito

                //DezNotas
                static void DezNotas()
                {

                }
                //DezNotas

                //SerieNumerica
                static void SerieNumerica()
                {

                }
                //SerieNumerica

                //volume de esfera
                static void Esfera()
                {
                    Console.WriteLine("qual o raio da esfera:");
                    double r = Convert.ToDouble(Console.ReadLine());
                    double formula;
                    formula = Math.PI * Math.Pow(r, 3) / 4;
                    Console.WriteLine($"o volume da esfera é {formula}");
                }
                //volume de esfera

                //SerieDez
                static void SerieDez()
                {
                    int n = 1;
                    do
                    {

                        Console.WriteLine($"{n}");
                        n++;

                    } while (n <= 10);
                }
                //SerieDez

                //MediaDeAluno
                static void MediaDeAluno()
                {

                }
                //MediaDeAluno

                //Expressão2
                static void ExpressãoH()
                {
                    {
                        int l = 0;
                        double dois, soma;
                        soma = 0;
                        do
                        {
                            Console.WriteLine("qual o tamanho da espressão");
                            double n = Convert.ToDouble(Console.ReadLine());
                            for (int i = 1; i <= n; i++)
                            {

                                dois = i;
                                soma += (1 / dois);
                                Console.WriteLine($"O valor {i} é {1}/ {dois}");
                            }
                            Console.WriteLine($" a soma dos valores da expressao de {n} é: {soma}");
                            Console.WriteLine($"digite 1 caso deseje parar");
                            int escolha = Convert.ToInt32(Console.ReadLine());
                            if (escolha == 1)
                            {
                                l = 1;
                            }
                            else
                            {

                            }
                        }
                        while (l == 0);
                    }
                }
                //Expressão2


                //Somatoria
                static void Somatoria()

                {
                    int l = 0;
                    double dois, um, soma;
                    soma = 0;
                    do
                    {
                        for (int i = 1; i <= 50; i++)
                        {
                            um = i;
                            dois = (2 * i - 1);
                            soma += (dois / um);
                            Console.WriteLine($"O valor {i} é {dois}/ {um}");
                        }
                        Console.WriteLine($"a soma dos valores é: {soma}");
                        Console.WriteLine($"digite 1 caso deseje parar");
                        int escolha = Convert.ToInt32(Console.ReadLine());
                        if (escolha == 1)
                        {
                            l = 1;
                        }
                        else
                        {

                        }
                    }
                    while (l == 0);
                }
                //Somatoria

                //Expressão
                static void Expressão()

                {
                    int l = 0;
                    double dois, um, soma;
                    soma = 0;
                    do
                    {

                        for (int i = 1; i <= 10; i++)
                        {
                            dois = (i * i);
                            um = i;
                            double fração = um / dois;
                            if (um % 2 != 0)
                            {
                                fração = -fração;
                            }
                            soma += fração;
                            Console.WriteLine($"O valor {i} é {um}/ {dois}");

                        }
                        Console.WriteLine($"a soma dos valores é: {soma}");
                        Console.WriteLine($"digite 1 caso deseje parar");
                        int escolha = Convert.ToInt32(Console.ReadLine());
                        if (escolha == 1)
                        {
                            l = 1;
                        }
                        else
                        {

                        }
                    }
                    while (l == 0);
                }
                //Expressão

                Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
                int escolha = Convert.ToInt32(Console.ReadLine());

                if (escolha == 2)
                {
                    i = 0;
                }
                else
                {

                }
            }
        }

