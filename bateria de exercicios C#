static void exercicio1()
{
    int i = 1;
    while (i > 0)
    {
        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - Volume de uma caixa retangular");
        Console.WriteLine("2 - Converter temperatura em graus Fahrenheit para Ceusius");
        Console.WriteLine("3 - Calcula volume de um cilindro");
        Console.WriteLine("4 - Curso de criação de coêlhos");
        Console.WriteLine("5 - Calcula consumo de combustível");
        Console.WriteLine("6 - Calcula o quadrado da diferença entre números");
        Console.WriteLine("7 - Propriedade distributiva entre 4 números");
        Console.WriteLine("8 - Calcula o volume de uma esfera");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                Caixa();
                break;

            case 2:
                Temperatura();
                break;

            case 3:
                Cilindro();
                break;

            case 4:
                Coelhos();
                break;

            case 5:
                Combustivel();
                break;

            case 6:
                Diferença();
                break;

            case 7:
                Distribuitiva();
                break;

            case 8:
                Esfera();
                break;
        }

        //caixa
        static void Caixa()
        {
            int altura, largura, profundidade;

            Console.WriteLine("qual a altura em metros da caixa");
            altura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a largura em metros da caixa");
            largura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a profundidade em metros da caixa");
            profundidade = Convert.ToInt32(Console.ReadLine());
            int formula;
            formula = largura * altura * profundidade;
            Console.WriteLine($" o volume da caixa é {formula} ");
        }
        //caixa


        //temperatura
        static void Temperatura()
        {
            float temperatura, c, f;
            char unidade;

            Console.WriteLine("qual a temperatura?");
            temperatura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a unidade de medida?");
            unidade = Char.ToUpper(Convert.ToChar(Console.ReadLine()));
            f = (9 * temperatura) / 5 + 32;
            c = (5 * (temperatura - 32)) / 9;
            if (unidade == 'C' || unidade == 'c')
            {
                Console.WriteLine($" a temperatura é {f}ºF");
            }

            if (unidade == 'F' || unidade == 'f')
            {
                Console.WriteLine($" a temperatura é {c}ºC");
            }

        }
        //temperatura

        //cilindro
        static void Cilindro()
        {
            double formula, altura, raio;

            Console.WriteLine("qual o raio do cilindro:");
            raio = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("qual a altura do cilindro:");
            altura = Convert.ToDouble(Console.ReadLine());

            formula = Math.PI * Math.Pow(raio, 2) * altura;

            Console.WriteLine($"o volume é {formula}");
        }
        //cilindro

        //coelhos
        static void Coelhos()
        {
            Console.WriteLine("Quantos coelhos possue ou vai comprar?");
            int Coelho = Convert.ToInt32(Console.ReadLine());
            int custoc = Coelho * 150;
            Console.WriteLine("O custo de um coelho varia de 50 a 200 reais");
            Console.WriteLine($"Já o custo mensal de seu(s) é de {custoc} e o gato total que ira fazer vai variar de {custoc * 12 + 50} a {custoc * 12 + 200}");
        }
        //coelhos

        //consumo
        static void Combustivel()
        {
            int distancia, litros;

            Console.WriteLine("qual a distancia percorrida?");
            distancia = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("em quanto litros?");
            litros = Convert.ToInt32(Console.ReadLine());
            int formula = distancia / litros;
            Console.WriteLine($"o consumo e de:{formula}");


        }
        //consumo

        //diferença expoenecial
        static void Diferença()
        {
            Console.WriteLine("digite o primeiro numero");
            double num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("digite o segundo numero");
            double num2 = Convert.ToInt32(Console.ReadLine());


            double diferenca = num1 - num2;
            double quadrado_da_diferenca = Math.Pow(diferenca, 2);


            Console.WriteLine($"O quadrado da diferença entre {num1} e {num2} é {quadrado_da_diferenca}");
        }
        //diferença expoenecial

        //Distribuitiva
        static void Distribuitiva()
        {
            Console.Write("Digite o valor de A: ");
            double a = Convert.ToDouble(Console.ReadLine());

            Console.Write("Digite o valor de B: ");
            double b = Convert.ToDouble(Console.ReadLine());

            Console.Write("Digite o valor de C: ");
            double c = Convert.ToDouble(Console.ReadLine());

            Console.Write("Digite o valor de D: ");
            double d = Convert.ToDouble(Console.ReadLine());
            double resultadoDireto = a * (b + c + d);

            Console.WriteLine($"{a}X ({b}+ {c}+ {d})");
            Console.WriteLine($"{resultadoDireto}");
        }
        //Distribuitiva

        //volume de esfera
        static void Esfera()
        {
            Console.WriteLine("qual o raio da esfera:");
            double r = Convert.ToDouble(Console.ReadLine());
            double formula = (4.0 / 3.0) * Math.PI * Math.Pow(r, 3);
            Console.WriteLine($"o volume da esfera é {formula}");
        }
        //volume de esfera

        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha = Convert.ToInt32(Console.ReadLine());

        if (escolha == 2)
        {
            i = 0;
        }
        else
        {

        }
    }
}
static void exercicio2()
{
    int i = 1;
    while (i > 0)
    {

        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - Calcula o produto e a adição de frações ");
        Console.WriteLine("2 - Calcula o salário de um vendedor");
        Console.WriteLine("3 - Calcula o volume de uma lata de oleo");
        Console.WriteLine("4 - Média harmônica");
        Console.WriteLine("5 - Média ponderada");
        Console.WriteLine("6 - Lê números e calcula operações");
        Console.WriteLine("7 - Calcula potência de número");
        Console.WriteLine("8 - Quadrado de um número");
        Console.WriteLine("9 - Área de um quadrado");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                ProdutoeAdicaoFracao();
                break;

            case 2:
                salario();
                break;

            case 3:
                VolumeLata();
                break;

            case 4:
                MediaHarmonica();
                break;

            case 5:
                MediaPonderada();
                break;

            case 6:
                LerNumeros();
                break;

            case 7:
                Potencia();
                break;

            case 8:
                AreaQuadrado();
                break;

            case 9:
                NumeroAoQuadrado();
                break;
        }

        //produto
        static void ProdutoeAdicaoFracao()
        {
            Console.WriteLine("qual o numerador?");
            double num1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("qual o denominador?");
            double num2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("qual o outro numerador?");
            double num3 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("qual o outro denominador?");
            double num4 = Convert.ToDouble(Console.ReadLine());
            double formula = num1 / num2;
            double formula2 = num3 / num4;
            Console.WriteLine($"A soma das fracões é {formula + formula2} O produto é {formula * formula2}");
        }
        //produto

        //salario
        static void salario()
        {

            Console.WriteLine("Qual a comissão?");
            double comissao = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Qual o salario?");
            double salario = Convert.ToDouble(Console.ReadLine());
            double formula = salario * comissao + salario;
            Console.WriteLine($"O salário total é R$ {formula}");
        }
        //salario

        //volumelata
        static void VolumeLata()
        {
            double formula, altura, raio;

            Console.WriteLine("qual o raio da lata de oleo:");
            raio = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("qual a altura da lata de oleo:");
            altura = Convert.ToDouble(Console.ReadLine());

            formula = Math.PI * Math.Pow(raio, 2) * altura;

            Console.WriteLine($"o volume é {formula}");
        }
        //volumelata

        //mediaHarmonica
        static void MediaHarmonica()
        {
            Console.WriteLine("qual a primeira nota");
            double nota1 = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("qual a segunda nota");
            double nota2 = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("qual a terceira nota");
            double nota3 = Convert.ToDouble(Console.ReadLine());

            double n = 3;

            double formula = n / ((1 / nota1) + (1 / nota2) + (1 / nota3));
            Console.WriteLine($" a media ponderada é {formula}");
        }
        //mediaHarmonica


        //mediaPonderada
        static void MediaPonderada()
        {
            Console.WriteLine("qual a primeira nota");
            int nota1 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual o peso da primeira nota");
            int peso1 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a segunda nota");
            int nota2 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual o peso segunda nota");
            int peso2 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual a terceira nota");
            int nota3 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual o peso terceira nota");
            int peso3 = Convert.ToInt32(Console.ReadLine());

            int formula = ((nota1 * peso1) + (nota2 * peso2) + (nota3 * peso3)) / (peso1 + peso2 + peso3);
            Console.WriteLine($" a media ponderada é {formula}");
        }
        //mediaPonderada

        //ler
        static void LerNumeros()
        {
            Console.WriteLine("qual o primeiro valor");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o segundo valor");
            int num2 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine($"A soma dos valores é:{num1 + num2}");
            Console.WriteLine($"A subtração é:{num1 - num2}");
            Console.WriteLine($"A multiplicação dos valores é:{num1 * num2}");
            Console.WriteLine($"A divisão dos valores é:{num1 / num2}");
        }
        //ler

        //Potencia
        static void Potencia()
        {
            Console.WriteLine("Digite o numero");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("digite o expoente");
            int expoente = Convert.ToInt32(Console.ReadLine());

            double resultado = Math.Pow(num1, expoente);
            Console.WriteLine($"a potencia é {resultado}");
        }
        //Potencia

        //area quadrado
        static void AreaQuadrado()
        {
            Console.WriteLine("Qual o comprimento do lado do quadrado?");
            double lado = Convert.ToDouble(Console.ReadLine());
            double area = Math.Pow(lado, 2);
            Console.WriteLine($"A área do quadrado é {area}");
        }
        //area quadrado

        //Numero Ao quadrado
        static void NumeroAoQuadrado()
        {
            Console.WriteLine("qual o numero?");
            double num = Convert.ToDouble(Console.ReadLine());
            double formula = num * num;
            Console.WriteLine($" a area e {formula}");
        }
        //Numero Ao quadrado

        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha = Convert.ToInt32(Console.ReadLine());

        if (escolha == 2)
        {
            i = 0;
        }
        else
        {

        }
    }
}


static void exercicio3()
{
    int i = 1;
    while (i > 0)
    {

        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - Consumo de combustível");
        Console.WriteLine("2 - Troca de valores entre números");
        Console.WriteLine("3 - Entrada e saída de dados");
        Console.WriteLine("4 - Conversão de temperaturas");
        Console.WriteLine("5 - Prestação em atrazo");
        Console.WriteLine("6 - Soma do quadrado de números");
        Console.WriteLine("7 - Salário de funcionário");
        Console.WriteLine("8 - Salário mensal");
        Console.WriteLine("9 - Velocidade de veículo");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                ConsumoCombustível();
                break;

            case 2:
                TrocaValor();
                break;

            case 3:
                AreaTriangulo();
                break;

            case 4:
                Temperatura();
                break;

            case 5:
                Prestação();
                break;

            case 6:
                SomaQuadrado();
                break;

            case 7:
                Funcionário();
                break;

            case 8:
                Mensal();
                break;

            case 9:
                Velocidade();
                break;
        }

        //ConsumoCombustível
        static void ConsumoCombustível()
        {
            Console.WriteLine("Quantos quilometros você rodou?");
            int Km = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("Qual o consumo medio de combustivel?");
            int lKm = Convert.ToInt32(Console.ReadLine());

            double formula = Km / lKm;

            Console.WriteLine($"a quantidade de litros consumida é de :{formula}L");
        }
        //ConsumoCombustível

        //TrocaValor
        static void TrocaValor()
        {
            Console.WriteLine("Digite o primeiro número:");
            int num1 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("Digite o segundo número:");
            int num2 = Convert.ToInt32(Console.ReadLine());

            // Trocando valores
            int temp = num1;
            num1 = num2;
            num2 = temp;

            Console.WriteLine($"Após a troca: num1 = {num1}, num2 = {num2}");
        }
        //TrocaValor

        //AreaTriangulo
        static void AreaTriangulo()
        {
            Console.WriteLine("qual a altura do triangulo");
            double altura = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("qual a base do triangulo");
            double bases = Convert.ToDouble(Console.ReadLine());
            double formula = (bases * altura) / 2;
            Console.WriteLine($" a area e {formula}");
        }
        //AreaTriangulo

        //temperatura
        static void Temperatura()
        {
            float temperatura, c, f;
            char unidade;

            Console.WriteLine("qual a temperatura?");
            temperatura = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual a unidade de medida?");
            unidade = Char.ToUpper(Convert.ToChar(Console.ReadLine()));
            f = (9 * temperatura) / 5 + 32;
            c = (5 * (temperatura - 32)) / 9;
            if (unidade == 'C' || unidade == 'c')
            {
                Console.WriteLine($" a temperatura é {f}");
            }

            if (unidade == 'F' || unidade == 'f')
            {
                Console.WriteLine($" a temperatura é {c}");
            }

        }
        //temperatura

        //Prestação
        static void Prestação()
        {
            Console.WriteLine("Qual o valor do prestação?");
            int aluguel = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Qual o juros?");
            double juros = Convert.ToDouble(Console.ReadLine());
            double formula = aluguel + juros;
            Console.WriteLine($"o valor total é: {formula}");
        }
        //Prestação

        //SomaQuadrado
        static void SomaQuadrado()
        {
            Console.WriteLine("qual o valor do primeiro numero");
            int num1 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual o valor do segundo numero");
            int num2 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("qual o valor do terceiro numero");
            int num3 = Convert.ToInt32(Console.ReadLine());

            int formula = (num1 * num1) + (num2 * num2) + (num3 * num3);

            Console.WriteLine($"a soma dos quadrados é: {formula}");
        }
        //SomaQuadrado

        //Funcionário
        static void Funcionário()
        {
            Console.WriteLine("Qual o salario?");
            int salario = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Qual os valores adicionais?");
            double adicionais = Convert.ToDouble(Console.ReadLine());
            double formula = salario * adicionais + salario;
            Console.WriteLine($"O salario é {formula}");
        }
        //Funcionário

        //Mensal
        static void Mensal()
        {
            Console.WriteLine("Qual a comissão?");
            double comissao = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Qual o salario?");
            int salario = Convert.ToInt32(Console.ReadLine());
            double formula = salario * comissao + salario;
            Console.WriteLine($"O salario é {formula}");
        }
        //Mensal

        // Velocidade
        static void Velocidade()
        {
            Console.WriteLine("Qual a distancia percorrida?");
            double deltaS = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Em quanto tempo?Em horas");
            double deltaT = Convert.ToDouble(Console.ReadLine());

            double formula = deltaS / deltaT;

            Console.WriteLine($"A velocidade do carro é de {formula}Km/h");
        }
        // Velocidade

        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha = Convert.ToInt32(Console.ReadLine());

        if (escolha == 2)
        {
            i = 0;
        }
        else
        {

        }
    }
    Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
    int escolha2 = Convert.ToInt32(Console.ReadLine());

    if (escolha2 == 2)
    {
        i = 0;

    }
    else
    {

    }
}

static void exercicio4()
{
    int i = 1;
    while (i > 0)
    {

        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - Verifica número informado");
        Console.WriteLine("2 - Verificar o maior e o menor número dados");
        Console.WriteLine("3 - Verificar se medidas informadas é um triângulo");
        Console.WriteLine("4 - Ler dois números e verificar a diferença");
        Console.WriteLine("5 - Aceita números e verifica seus divisores");
        Console.WriteLine("6 - Lê dois número e verifica suas diferenças");
        Console.WriteLine("7 - Calcula média de aluno");
        Console.WriteLine("8 - Lê números e compara valores");
        Console.WriteLine("9 - Verifica se aluno foi aprovado");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                Verificacao();
                break;

            case 2:
                ComparacoNumeros();
                break;

            case 3:
                Triangulo();
                break;

            case 4:
                Diferenca();
                break;

            case 5:
                AceitaNumeros();
                break;

            case 6:
                Diferenca();
                break;

            case 7:
                MediaAluno();
                break;

            case 8:
                ComparaValores();
                break;

            case 9:
                EX9();
                break;
        }
        static void Verificacao()
        {
            int numero;

            Console.WriteLine("digite um número:");
            numero = Convert.ToInt32(Console.ReadLine());

            if (numero < 0)
            {
                Console.WriteLine("O número está abaixo de 0.");
            }
            else if (numero >= 0 && numero <= 10)
            {
                Console.WriteLine("O número está entre 0 e 10.");
            }
            else if (numero > 10 && numero <= 100)
            {
                Console.WriteLine("O número está entre 11 e 100.");
            }
            else
            {
                Console.WriteLine("O número está acima de 100.");
            }
        }

        //comparaconumeros
        static void ComparacoNumeros()
        {
            Console.WriteLine("qual o primeiro numero?");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o segundo numero?");
            int num2 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o terceiro numero?");
            int num3 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o quarto numero?");
            int num4 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o quinto numero?");
            int num5 = Convert.ToInt32(Console.ReadLine());

            if (num1 > num2 && num1 > num3 && num1 > num4 && num1 > num5)
            {
                Console.WriteLine($"{num2} é o maior");
            }
            if (num1 < num2 && num1 < num3 && num1 < num2 && num1 < num5)
            {
                Console.WriteLine($"{num1} é o menor");
            }
            if (num2 > num1 && num2 > num3 && num2 > num4 && num2 > num5)
            {
                Console.WriteLine($"{num1} é o maior");
            }
            if (num2 < num1 && num2 < num3 && num2 < num4 && num2 < num5)
            {
                Console.WriteLine($"{num2} é o menor");
            }
            if (num3 > num1 && num3 > num2 && num3 > num4 && num3 > num5)
            {
                Console.WriteLine($"{num3} é o maior");
            }
            if (num3 < num1 && num3 < num4 && num3 < num2 && num3 < num5)
            {
                Console.WriteLine($"{num3} é o menor");
            }
            if (num4 > num1 && num4 > num3 && num4 > num2 && num4 > num5)
            {
                Console.WriteLine($"{num4} é o maior");
            }
            if (num4 < num1 && num4 < num3 && num4 < num2 && num4 < num5)
            {
                Console.WriteLine($"{num4} é o menor");
            }
            if (num5 > num1 && num5 > num3 && num5 > num2 && num5 > num4)
            {
                Console.WriteLine($"{num5} é o maior");
            }
            if (num5 < num1 && num5 < num3 && num5 < num2 && num5 < num4)
            {
                Console.WriteLine($"{num5} é o menor");
            }
        }
        //comparaconumeros

        static void Triangulo()
        {
            double a, b, c;

            Console.WriteLine("Informe o primeiro lado do triângulo:");
            a = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Informe o segundo lado do triângulo:");
            b = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Informe o terceiro lado do triângulo:");
            c = Convert.ToDouble(Console.ReadLine());

            if (a + b > c && a + c > b && b + c > a)
            {
                Console.WriteLine("É um triângulo!");
                if (a == b && b == c)
                {
                    Console.WriteLine("Tipo: Equilátero");
                }
                else if (a == b || a == c || b == c)
                {
                    Console.WriteLine("Tipo: Isósceles");
                }
                else
                {
                    Console.WriteLine("Tipo: Escaleno");
                }
            }
            else
            {
                Console.WriteLine("As medidas informadas não formam um triângulo.");
            }

        }

        //AceitaNumeros
        static void AceitaNumeros()
        {
            int quantidade = 4;
            int[] numeros = new int[quantidade];

            for (int i = 0; i < 4; i++)
            {
                Console.Write($"Digite o {i + 1}º número: ");
                numeros[i] = Convert.ToInt32(Console.ReadLine());
            }

            foreach (int num in numeros)
            {
                if (num % 2 == 0)
                {
                    Console.WriteLine($"{num} é divisível por 2");
                }
                else
                {
                    Console.WriteLine($"{num} não é divisível por 2");
                }
                if (num % 3 == 0)
                {
                    Console.WriteLine($"{num} é divisível por 3");
                }
                else
                {
                    Console.WriteLine($"{num} não é divisível por 3");
                }
            }
        }
        //AceitaNumeros

        // Diferenca
        static void Diferenca()
        {
            Console.WriteLine("qual o primeiro numero?");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o segundo numero?");
            int num2 = Convert.ToInt32(Console.ReadLine());
            int formula = num1 - num2;
            if (num1 > num2)
            {
                Console.WriteLine($"o maior numero e {num1}");
                Console.WriteLine($"a diferenca e{formula}");
            }

            else
            {
                Console.WriteLine($"o maior numero e {num2}");
                Console.WriteLine($"a diferenca e{formula}");
            }
        }
        //Diferenca

        //MediaAluno
        static void MediaAluno()
        {
            Console.WriteLine("qual o primeira nota?");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o segunda nota?");
            int num2 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o terceira nota?");
            int num3 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("qual o quarta nota?");
            int num4 = Convert.ToInt32(Console.ReadLine());
            int formula = (num1 + num2 + num3 + num4) / 4;
            Console.WriteLine($"a media e {formula}");
            int Media = 7;
            if (formula >= Media)
            {
                Console.WriteLine("aluno aprovado");
            }

            else
            {
                Console.WriteLine("aluno reprovado");
            }
        }
        //MediaAluno

        static void ComparaValores()
        {
            double num1, num2, num3;

            Console.WriteLine("Informe o 1º número:");
            num1 = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Informe o 2º número:");
            num2 = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Informe o 3º número:");
            num3 = Convert.ToDouble(Console.ReadLine());


            if (num1 < 10)
            {
                num2 *= 2;
                num3 += 5;
            }
            else if (num1 >= 10 && num1 <= 50)
            {
                num2 += 10;
                num3 -= 3;
            }
            else
            {
                num2 /= 2;
                num3 *= 3;
            }

            Console.WriteLine($"\nValores atualizados:");
            Console.WriteLine($"2º número: {num2}");
            Console.WriteLine($"3º número: {num3}");
        }

        static void EX9()
        {
            Console.WriteLine("digite as sua media");
            int media = Convert.ToInt32(Console.ReadLine());
            switch (media)
            {
                case 1:
                    Console.WriteLine("reprovado");
                    Console.WriteLine("conceito E");
                    break;
                case 2:
                    Console.WriteLine("reprovado");
                    Console.WriteLine("conceito E");
                    break;
                case 3:
                    Console.WriteLine("reprovado");
                    Console.WriteLine("conceito D");
                    break;
                case 4:
                    Console.WriteLine("reprovado");
                    Console.WriteLine("conceito D");
                    break;
                case 5:
                    Console.WriteLine("reprovado");
                    Console.WriteLine("conceito C");
                    break;
                case 6:
                    Console.WriteLine("reprovado");
                    Console.WriteLine("conceito C");
                    break;
                case 7:
                    Console.WriteLine("aprovado");
                    Console.WriteLine("conceito B");
                    break;
                case 8:
                    Console.WriteLine("aprovado");
                    Console.WriteLine("conceito B");
                    break;
                case 9:
                    Console.WriteLine("aprovado");
                    Console.WriteLine("conceito A");
                    break;
                case 10:
                    Console.WriteLine("aprovado");
                    Console.WriteLine("conceito A");
                    break;
            }

        }


        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha = Convert.ToInt32(Console.ReadLine());

        if (escolha == 2)
        {
            i = 0;
        }
        else
        {

        }
    }
}

static void exercicio5()
{
    int i = 1;
    while (i > 0)
    {

        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - Soma números e compara resultado");
        Console.WriteLine("2 - Reajusta salário");
        Console.WriteLine("3 - Lê número e o converte de + para -");
        Console.WriteLine("4 - Acha intervado de número");
        Console.WriteLine("5 - Soma funções");
        Console.WriteLine("6 - Recebe números e verifica divisores");
        Console.WriteLine("7 - Verifica lados de um triângulo");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                EX1();
                break;

            case 2:
                EX2();
                break;

            case 3:
                EX3();
                break;

            case 4:
                EX4();
                break;

            case 5:
                EX5();
                break;

            case 6:
                EX6();
                break;

            case 7:
                EX7();
                break;
        }

        static void EX1()
        {
            int valor = 0;
            Console.WriteLine("digite o primeiro numero");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("digite o segundo numero");
            int num2 = Convert.ToInt32(Console.ReadLine());

            valor = num1 + num2;
            if (valor > 10)
            {
                Console.WriteLine("maior que 10");
            }
            else
            {

            }
        }

        static void EX2()
        {
            Console.WriteLine("digite o salario");
            int salario = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("o salario foi ajustado em quantos por cento?");
            double reajuste = Convert.ToDouble(Console.ReadLine());

            double formula = (salario * (reajuste / 100)) + salario;

            if (formula > salario)
            {
                Console.WriteLine($"O salario aumentou de {salario} para {formula}");
            }

            if (formula < salario)
            {
                Console.WriteLine($"O salario diminuiu de {salario} para {formula}");
            }

            else
            {
                Console.WriteLine("o salario não mudou");
            }
        }

        static void EX3()
        {
            Console.WriteLine("Digite o numero");
            int num = Convert.ToInt32(Console.ReadLine());
            if (num < 0)
            {
                Console.WriteLine($"deixando o número positivo {num * -1}");
            }
            else
            {
                Console.WriteLine("número já é positivo");
            }
        }

        static void EX4()
        {
            Console.WriteLine("Digite o valor de A:");
            int A = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Digite o valor de B:");
            int B = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Digite o valor de C:");
            int C = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Digite o valor de D:");
            int D = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("Digite um número");
            int num = Convert.ToInt32(Console.ReadLine());

            if (num >= A && num <= B && num >= C && num <= D)
            {
                Console.WriteLine("número em ambos intervalos");
            }

            else
            {
                if (num >= A && num <= B)
                {
                    Console.WriteLine("número em intervalo[AB]");
                }

                if (num >= C && num <= D)
                {
                    Console.WriteLine("número em intervalo[CD]");
                }

                else if (num < C || num > B && num < A || num > D)
                {
                    Console.WriteLine("número em nenhum intervalo");
                }
            }
        }

        static void EX5()
        {
            double f(double x)
            {
                return 2 * Math.Pow(x, 2) - 4 * x + 12;
            }

            double g(double x)
            {
                return 1 * Math.Pow(x, 2) - 8 * x - 10;
            }

            Console.WriteLine("Digite um valor para x:");
            double x = Convert.ToDouble(Console.ReadLine());

            double formula = f(x) + g(x);
            Console.WriteLine($"o resultado é: {formula}");
        }

        static void EX6()
        {
            int quantidade = 4;
            int[] numeros = new int[quantidade];

            for (int i = 0; i < 4; i++)
            {
                Console.Write($"Digite o {i + 1}º número: ");
                numeros[i] = Convert.ToInt32(Console.ReadLine());
            }

            foreach (int num in numeros)
            {
                if (num % 2 == 0)
                {
                    Console.WriteLine($"{num} é divisível por 2");
                }
                else
                {
                    Console.WriteLine($"{num} não é divisível por 2");
                }
                if (num % 3 == 0)
                {
                    Console.WriteLine($"{num} é divisível por 3");
                }
                else
                {
                    Console.WriteLine($"{num} não é divisível por 3");
                }
            }
        }

        static void EX7()
        {
            double a, b, c;

            Console.WriteLine("Digite o lado A:");
            a = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Digite o lado B:");
            b = Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("Digite o lado C:");
            c = Convert.ToDouble(Console.ReadLine());


            double maior = a, meio = b, menor = c;

            if (maior < meio)
            {
                double temp = maior;
                maior = meio;
                meio = temp;
            }

            if (maior < menor)
            {
                double temp = maior;
                maior = menor;
                menor = temp;
            }

            if (meio < menor)
            {
                double temp = meio;
                meio = menor;
                menor = temp;
            }

            Console.WriteLine($"\nLados em ordem decrescente: {maior}, {meio}, {menor}");


            if (maior < meio + menor)
            {
                Console.WriteLine("Forma um triângulo.");

                if (a == b && b == c)
                    Console.WriteLine("Tipo: Triângulo Equilátero.");
                else if (a == b || b == c || a == c)
                    Console.WriteLine("Tipo: Triângulo Isósceles.");
                else
                    Console.WriteLine("Tipo: Triângulo Escaleno.");
            }
            else
            {
                Console.WriteLine("Não forma um triângulo.");
            }
        }
    }

    Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
    int escolha = Convert.ToInt32(Console.ReadLine());

    if (escolha == 2)
    {
        i = 0;
    }
    else
    {

    }
}

static void exercicio6()
{
    int i = 1;
    while (i > 0)
    {

        Console.WriteLine("qual o exercicio");
        Console.WriteLine("1 - Verifica nome e sexo de pessoa");
        Console.WriteLine("2 - Compara soma de números");
        Console.WriteLine("3 - Extrai raízes quadradas");
        Console.WriteLine("4 - Compara valores");
        Console.WriteLine("5 - Calcula Imposto de Renda");
        Console.WriteLine("6 - Reajusta salário");
        Console.WriteLine("7 - Verifica número");
        int escolha1 = Convert.ToInt32(Console.ReadLine());
        switch (escolha1)
        {
            case 1:
                EX1();
                break;

            case 2:
                EX2();
                break;

            case 3:
                EX3();
                break;

            case 4:
                EX4();
                break;

            case 5:
                EX5();
                break;

            case 6:
                EX6();
                break;

            case 7:
                EX7();
                break;
        }

        static void EX1()
        {
            Console.WriteLine("digite o seu nome");
            string nome = Console.ReadLine();

            Console.WriteLine("digite o seu sexo");
            string sexo = Console.ReadLine();

            Console.WriteLine($"seu nome é:{nome} e seu sexo é:{sexo}");

        }

        static void EX2()
        {
            Console.WriteLine("digite o primeiro numero");
            int num1 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("digite o primeiro numero");
            int num2 = Convert.ToInt32(Console.ReadLine());

            int soma = num1 + num2;

            if (soma > 10)
            {
                int formula = soma + 5;
                Console.WriteLine($"soma dá mais que dez resultado:{formula}");
            }

            else
            {
                int formula = soma - 7;
                Console.WriteLine($"soma dá menos que dez resultado:{formula}");
            }

        }

        static void EX3()
        {
            Console.WriteLine("digite o a da função");
            int a = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("digite o b da função");
            int b = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("digite o c da função");
            int c = Convert.ToInt32(Console.ReadLine());

            double delta = Math.Pow(b, 2) - 4 * a * c;
            double x1 = (-b + Math.Sqrt(delta)) / (2 * a);
            double x2 = (-b - Math.Sqrt(delta)) / (2 * a);

            if (delta < 0)
            {
                Console.WriteLine("não há raizes");
            }
            Console.WriteLine($"x1 = {x1}");
            Console.WriteLine($"x2 = {x2}");
        }

        static void EX4()
        {
            Console.WriteLine("Digite o primeiro número:");
            int num1 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("Digite o segundo número:");
            int num2 = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("Digite o terceiro número:");
            int num3 = Convert.ToInt32(Console.ReadLine());

            int maior, medio, menor;


            if (num1 >= num2 && num1 >= num3)
            {
                maior = num1;
                if (num2 >= num3)
                {
                    medio = num2;
                    menor = num3;
                }
                else
                {
                    medio = num3;
                    menor = num2;
                }
            }
            else if (num2 >= num1 && num2 >= num3)
            {
                maior = num2;
                if (num1 >= num3)
                {
                    medio = num1;
                    menor = num3;
                }
                else
                {
                    medio = num3;
                    menor = num1;
                }
            }
            else
            {
                maior = num3;
                if (num1 >= num2)
                {
                    medio = num1;
                    menor = num2;
                }
                else
                {
                    medio = num2;
                    menor = num1;
                }
            }

            Console.WriteLine($"Maior: {maior}");
            Console.WriteLine($"Médio: {medio}");
            Console.WriteLine($"Menor: {menor}");


        }

        static void EX5()
        {
            Console.WriteLine("digite o salario");
            double salario = Convert.ToDouble(Console.ReadLine());

            if (salario <= 2259.2)
            {
                Console.WriteLine($"o imposto de renda é nulo");
            }

            if (salario > 2259.2)
            {
                if (salario <= 2826.65)
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.075}");
                }
                if (salario > 2826.65 && salario <= 3751.05)
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.15}");
                }

                else if (salario <= 4664.68)
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.225}");
                }

                else
                {
                    Console.WriteLine($"o imposto de renda é {salario * 0.275}");
                }
            }

        }

        static void EX6()
        {
            Console.WriteLine("digite o salario");
            int salario = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("o salario foi ajustado em quantos por cento?");
            double reajuste = Convert.ToDouble(Console.ReadLine());

            double formula = (salario * (reajuste / 100)) + salario;

            if (formula > salario)
            {
                Console.WriteLine($"O salario aumentou de {salario} para {formula}");
            }

            if (formula < salario)
            {
                Console.WriteLine($"O salario diminuiu de {salario} para {formula}");
            }

            else
            {
                Console.WriteLine("o salario não mudou");
            }
        }

        static void EX7()
        {
            Console.WriteLine("Digite um número:");
