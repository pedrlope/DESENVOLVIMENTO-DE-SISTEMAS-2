            //EX2
            static void EX2()
            {
                Console.WriteLine("Digite 1 para media harmonica, 2 para media aritimedica e 3 para media geometrica");
                int mediaescolha = Convert.ToInt32(Console.ReadLine);
                switch (mediaescolha)
                {

                    case 1:
                        Console.WriteLine("qual a primeira nota");
                        int nota1 = Convert.ToInt32(Console.ReadLine());

                        Console.WriteLine("qual a segunda nota");
                        int nota2 = Convert.ToInt32(Console.ReadLine());

                        Console.WriteLine("qual a terceira nota");
                        int nota3 = Convert.ToInt32(Console.ReadLine());

                        int n = 3;

                        int formula = n / ((1 / nota1) + (1 / nota2) + (1 / nota3));
                        Console.WriteLine($" a media aritimedica é {formula}");
                        break;

                    case 2:
                        Console.WriteLine("qual a primeira nota");
                        int nota4 = Convert.ToInt32(Console.ReadLine());

                        Console.WriteLine("qual a segunda nota");
                        int nota5 = Convert.ToInt32(Console.ReadLine());

                        Console.WriteLine("qual a terceira nota");
                        int nota6 = Convert.ToInt32(Console.ReadLine());

                        int formula2 = (nota4 + nota5 + nota6) / 3;
                        Console.WriteLine($" a media ponderada é {formula2}");
                        break;

                    case 3:
                        Console.WriteLine("qual a primeira nota");
                        int nota7 = Convert.ToInt32(Console.ReadLine());

                        Console.WriteLine("qual a segunda nota");
                        int nota8 = Convert.ToInt32(Console.ReadLine());

                        Console.WriteLine("qual a terceira nota");
                        int nota9 = Convert.ToInt32(Console.ReadLine());

                        double formula3 = Math.Pow(nota7 * nota8 * nota9, 1.0 / 3.0);
                        Console.WriteLine($" a media ponderada é {formula3}");
                        break;
                }
            }
            //EX2

            //EX3
            static void EX3()
            {
                bool wheli = true;
                do
                {
                    Console.WriteLine("Qual a sua linguagem da programação");
                    string programacao = Console.ReadLine();
                    switch (programacao)
                    {
                        case "C":
                            Console.WriteLine("Escreva uma linha no seu programa");
                            String C = Console.ReadLine();
                            if (C == "Printf" || C == "Console.WriteLine()" || C == "Print")
                            {
                                wheli = false;
                                Console.WriteLine("Parabéns!!");
                            }
                            else
                            {
                                Console.WriteLine("Isso não esta certo");
                            }
                            break;

                        case "Python":
                            Console.WriteLine("Escreva uma linha no seu programa");
                            String Python = Console.ReadLine();
                            if (Python == "print" || Python == "format" || Python == "f-strings")
                            {
                                Console.WriteLine("Parabéns!!");
                                wheli = false;
                            }

                            else
                            {
                                Console.WriteLine("Isso não esta certo");
                            }
                            break;

                        case "Java":
                            Console.WriteLine("Escreva uma linha no seu programa");
                            String Java = Console.ReadLine();
                            if (Java == "System.out.printf")
                            {
                                wheli = false;
                                Console.WriteLine("Parabéns!!");
                            }
                            else
                            {
                                Console.WriteLine("Isso não esta certo");
                            }
                            break;
                    }
                }
                while (wheli == true);
            }
            //EX3

            static void EX4()
            {
                Console.Write("Digite sua idade: ");
                int idade = Convert.ToInt32(Console.ReadLine());

                if (idade >= 18)
                {
                    Console.WriteLine("Você é maior de idade.");
                }
                else
                {
                    Console.WriteLine("Você é menor de idade.");
                }

                Console.WriteLine("Escolha uma opção:");
                Console.WriteLine("1 - Ver mensagem");
                Console.WriteLine("2 - Ver número");
                Console.WriteLine("3 - Sair");
                int opcao = Convert.ToInt32(Console.ReadLine());

                switch (opcao)
                {
                    case 1:
                        Console.WriteLine("Você escolheu ver a mensagem.");
                        break;
                    case 2:
                        Console.WriteLine("Você escolheu ver o número.");
                        break;
                    case 3:
                        Console.WriteLine("Saindo...");
                        break;
                    default:
                        Console.WriteLine("Opção inválida.");
                        break;
                }

                int contador1 = 0;
                Console.WriteLine("Contando com while:");
                while (contador1 < 3)
                {
                    Console.WriteLine($"Contador1: {contador1}");
                    contador1++;
                }
                int contador2 = 0;
                Console.WriteLine("Contando com do/while:");
                do
                {
                    Console.WriteLine($"Contador2: {contador2}");
                    contador2++;
                } while (contador2 < 3);

                Console.WriteLine("Contando com for:");
                for (int i = 0; i < 3; i++)
                {
                    Console.WriteLine($"i: {i}");
                }
            }

            static void EX5e8()
            {
                bool boll = true;

                do
                {
                    Console.WriteLine("quantos numeros vão ser rodados?");
                    int numrodado = Convert.ToInt32(Console.ReadLine());
                    for (int i = 0; i <= numrodado; i++)
                    {
                        Console.WriteLine($"\n{i}");
                        Console.WriteLine("qual foi número digitado?");
                        int num = Convert.ToInt32(Console.ReadLine());
                        if (num != i)
                        {
                            boll = false;
                            break;
                        }
                    }
                } while (boll);
            }

            static void EX6()
            {
                bool par = true;
                while (par)
                {
                    Console.WriteLine("Digite o primeiro número");
                    int num1 = Convert.ToInt32(Console.ReadLine());

                    Console.WriteLine("Digite o primeiro número");
                    int num2 = Convert.ToInt32(Console.ReadLine());

                    int diferença = num1 - num2;
                    if (num1 > num2)
                    {
                        Console.WriteLine($"{num1}é maior que {num2}");
                    }

                    if (num1 == num2)
                    {
                        Console.WriteLine("os valores são iguais");
                    }

                    else if (num2 > num1)
                    {
                        Console.WriteLine($"{num2}é maior que {num1}");
                    }


                    Console.WriteLine("quer parar?Digite 1 para parar");
                    int parar = Convert.ToInt32(Console.ReadLine());
                    if (parar == 1)
                    {
                        par = false;
                    }
                }
            }

            static void EX7()
            {
                for (int i = 0; i <= 3; i++)
                {

                    Console.WriteLine("qual o seu salario?");
                    int salario = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("quantos anos está na empresa?");
                    int em = Convert.ToInt32(Console.ReadLine());
                    switch (em)
                    {
                        case 10:
                            Console.WriteLine("qual o seu sexo?");
                            string x = Console.ReadLine();
                            switch (x)
                            {
                                case "mulher":
                                    Console.WriteLine("Seu bonus salarial é de 25%");
                                    Console.WriteLine($"o total é: {salario * 0.25 + salario}");
                                    break;

                                case "homem":
                                    Console.WriteLine("Seu bonus salarial é de 50R$");
                                    Console.WriteLine($"o total é: {salario + 50}");
                                    break;
                            }
                            break;



                        case >= 15:
                            Console.WriteLine("qual o seu sexo?");
                            string y = Console.ReadLine();
                            switch (y)
                            {
                                case "mulher":
                                    Console.WriteLine("Seu bonus salarial é de 25%");
                                    Console.WriteLine($"o total é: {salario * 0.25 + salario}");
                                    break;

                                case "homem":
                                    Console.WriteLine("Seu bonus salarial é de 20%");
                                    Console.WriteLine($"o total é: {salario * 0.2 + salario}");
                                    break;
                            }
                            break;

                        case < 10:
                            Console.WriteLine("Seu bonus salarial é de 50R$");
                            Console.WriteLine($"o total é: {salario + 50}");
                            break;
                    }
                }

            }

            //CaluloDeAreas
            static void EX9()
            {
                Console.WriteLine("voce deseja calcular a  area de qual forma?");
                Console.WriteLine("1 – Cubo");
                Console.WriteLine("2 – Hexagono");
                Console.WriteLine("3 – triangulo");
                int escolher = Convert.ToInt32(Console.ReadLine());
                if (escolher == 1)
                {
                    Console.WriteLine("qual o valor do primeiro lado do cubo?");
                    int l1 = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("qual o valor do segundo lado do cubo?");
                    int l2 = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("qual o valor do terceiro lado do cubo?");
                    int l3 = Convert.ToInt32(Console.ReadLine());
                    int formula = 6 * (l1 * l1);
                    Console.WriteLine($"A area do cubo é de : {formula}m²");

                }

                if (escolher == 2)
                {
                    Console.WriteLine("Qual o valor do comprimento de um lado do hexagono?");
                    int l1 = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("Qual o valor do apotema(meio do hexagono até um dos lados)?");
                    int apotema = Convert.ToInt32(Console.ReadLine());
                    int formula = (3 / 2) * apotema * l1;
                    Console.WriteLine($"A area do hexagono é de : {formula}m²");
                }

                if (escolher == 3)
                {
                    Console.WriteLine("qual o valor da base do triangulo?");
                    int bases = Convert.ToInt32(Console.ReadLine());
                    Console.WriteLine("qual a altura?");
                    int altura = Convert.ToInt32(Console.ReadLine());
                    int formula = (altura * bases) / 2;
                    Console.WriteLine($"A area do triangulo é de : {formula}m²");
                }

                else
                {
                    Console.WriteLine("Não é possivel");
                }
            }

            static void EX10()
            {
                int candidatoX = 0, candidatoY = 0, nulos = 0;
                Console.WriteLine("qual candidado você votou?Digite 0 para voto nulo, 1 para o candidatoX e 2 para o candidatoY");
                int voto = Convert.ToInt32(Console.ReadLine());
                switch (voto)
                {
                    case 0:
                        nulos++;
                        break;
                    case 1:
                        candidatoX++;
                        break;
                    case 2:
                        candidatoY++;
                        break;
                    default:
                        Console.WriteLine("Candidato inválido.");
                        return;
                }

                if (candidatoX > candidatoY)
                {
                    Console.WriteLine("O com a maior quantidade de votos foi candidatoX");
                }

                if (candidatoY > candidatoX)
                {
                    Console.WriteLine("O com a maior quantidade de votos foi candidatoY");
                }

                else
                {
                    Console.WriteLine("Nenhum dos candidatos teve a maioria dos votos");
                }

                Console.WriteLine($"O candidatoX teve {candidatoX} votos");
                Console.WriteLine($"O candidatoY teve {candidatoY} votos");
                Console.WriteLine($"Teve {nulos} votos nulos");

            }
        }
    }

    static void VerificarCompra(Produto produto, decimal dinheiro)
    {
        if (dinheiro >= produto.Preco)
        {
            Console.WriteLine($"Você pode comprar um {produto.Nome}");

            if ((dinheiro / 2) >= produto.Preco)
            {
                Console.WriteLine($"Você pode comprar mais de um {produto.Nome}");
            }
        }
        else
        {
            Console.WriteLine($"Você não tem dinheiro suficiente para comprar um {produto.Nome}");
        }
    }


class Produto
{
    public string Nome { get; set; }
    public decimal Preco { get; set; }

    public Produto(string nome, decimal preco)
    {
        Nome = nome;
        Preco = preco;
    }
}
