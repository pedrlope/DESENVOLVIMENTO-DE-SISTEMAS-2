        static void EX1()

        {
            int l = 0;
            double dois, um, soma;
            soma = 0;
            do
            {
                for (int i = 1; i <= 50; i++)
                {
                    um = i;
                    dois = (2 * i - 1);
                    soma += (dois / um);
                    Console.WriteLine($"O valor {i} é {dois}/ {um}");
                }
                Console.WriteLine($"a soma dos valores é: {soma}");
                Console.WriteLine($"digite 1 caso deseje parar");
                int escolha = Convert.ToInt32(Console.ReadLine());
                if (escolha == 1)
                {
                    l = 1;
                }
                else
                {

                }
            }
            while (l == 0);
        }
