    Console.WriteLine("escolha o exercicio");
    Console.WriteLine("1 – Entrada e Saída de Dados e Declaração de Variáveis");
    Console.WriteLine("2 – Entradas e Saídas de Dados e Declaração de Variáveis");
    Console.WriteLine("3 – Entrada e Saída de Dados e Declaração de Variáveis");
    int exercicio = Convert.ToInt32(Console.ReadLine());

    if (exercicio == 1)
    {
        exercicio1();
    }

    if (exercicio == 2)
    {
        exercicio2();
    }

    if (exercicio == 3)
    {
        exercicio3();
    }

    static void exercicio1()
    {
        int i = 1;
        while (i > 0)
        {
            Console.WriteLine("qual o exercicio");
            Console.WriteLine("1 - Volume de uma caixa retangular");
            Console.WriteLine("2 - Converter temperatura em graus Fahrenheit para Ceusius");
            Console.WriteLine("3 - Calcula volume de um cilindro");
            Console.WriteLine("4 - Curso de criação de coêlhos");
            Console.WriteLine("5 - Calcula consumo de combustível");
            Console.WriteLine("6 - Calcula o quadrado da diferença entre números");
            Console.WriteLine("7 - Propriedade distributiva entre 4 números");
            Console.WriteLine("8 - Calcula o volume de uma esfera");
            int escolha1 = Convert.ToInt32(Console.ReadLine());
            switch (escolha1)
            {
                case 1:
                    Caixa();
                    break;

                case 2:
                    Temperatura();
                    break;

                case 3:
                    Cilindro();
                    break;

                case 4:
                    Coelhos();
                    break;

                case 5:
                    Combustivel();
                    break;

                case 6:
                    Diferença();
                    break;

                case 7:
                    Distribuitiva();
                    break;

                case 8:
                    Esfera();
                    break;
            }

            //caixa
            static void Caixa()
            {
                int altura, largura, profundidade;

                Console.WriteLine("qual a altura em metros da caixa");
                altura = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("qual a largura em metros da caixa");
                largura = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("qual a profundidade em metros da caixa");
                profundidade = Convert.ToInt32(Console.ReadLine());
                int formula;
                formula = largura * altura * profundidade;
                Console.WriteLine($" o volume da caixa é {formula} ");
            }
            //caixa


            //temperatura
            static void Temperatura()
            {
                float temperatura, c, f;
                char unidade;

                Console.WriteLine("qual a temperatura?");
                temperatura = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("qual a unidade de medida?");
                unidade = Char.ToUpper(Convert.ToChar(Console.ReadLine()));
                f = (9 * temperatura) / 5 + 32;
                c = (5 * (temperatura - 32)) / 9;
                if (unidade == 'C' || unidade == 'c')
                {
                    Console.WriteLine($" a temperatura é {f}ºF");
                }

                if (unidade == 'F' || unidade == 'f')
                {
                    Console.WriteLine($" a temperatura é {c}ºC");
                }

            }
            //temperatura

            //cilindro
            static void Cilindro()
            {
                double formula, altura, raio;

                Console.WriteLine("qual o raio do cilindro:");
                raio = Convert.ToDouble(Console.ReadLine());

                Console.WriteLine("qual a altura do cilindro:");
                altura = Convert.ToDouble(Console.ReadLine());

                formula = Math.PI * Math.Pow(raio, 2) * altura;

                Console.WriteLine($"o volume é {formula}");
            }
            //cilindro

            //coelhos
            static void Coelhos()
            {
                Console.WriteLine("Quantos coelhos possue ou vai comprar?");
                int Coelho = Convert.ToInt32(Console.ReadLine());
                int custoc = Coelho * 150;
                Console.WriteLine("O custo de um coelho varia de 50 a 200 reais");
                Console.WriteLine($"Já o custo mensal de seu(s) é de {custoc} e o gato total que ira fazer vai variar de {custoc * 12 + 50} a {custoc * 12 + 200}");
            }
            //coelhos

            //consumo
            static void Combustivel()
            {
                int distancia, litros;

                Console.WriteLine("qual a distancia percorrida?");
                distancia = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("em quanto litros?");
                litros = Convert.ToInt32(Console.ReadLine());
                int formula = distancia / litros;
                Console.WriteLine($"o consumo e de:{formula}");


            }
            //consumo

            //diferença expoenecial
            static void Diferença()
            {
                Console.WriteLine("digite o primeiro numero");
                double num1 = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("digite o segundo numero");
                double num2 = Convert.ToInt32(Console.ReadLine());


                double diferenca = num1 - num2;
                double quadrado_da_diferenca = Math.Pow(diferenca, 2);


                Console.WriteLine($"O quadrado da diferença entre {num1} e {num2} é {quadrado_da_diferenca}");
            }
            //diferença expoenecial

            //Distribuitiva
            static void Distribuitiva()
            {
                Console.Write("Digite o valor de A: ");
                double a = Convert.ToDouble(Console.ReadLine());

                Console.Write("Digite o valor de B: ");
                double b = Convert.ToDouble(Console.ReadLine());

                Console.Write("Digite o valor de C: ");
                double c = Convert.ToDouble(Console.ReadLine());

                Console.Write("Digite o valor de D: ");
                double d = Convert.ToDouble(Console.ReadLine());
                double resultadoDireto = a * (b + c + d);

                Console.WriteLine($"{a}X ({b}+ {c}+ {d})");
                Console.WriteLine($"{resultadoDireto}");
            }
            //Distribuitiva

            //volume de esfera
            static void Esfera()
            {
                Console.WriteLine("qual o raio da esfera:");
                double r = Convert.ToDouble(Console.ReadLine());
                double formula = (4.0 / 3.0) * Math.PI * Math.Pow(r, 3);
                Console.WriteLine($"o volume da esfera é {formula}");
            }
            //volume de esfera

            Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
            int escolha = Convert.ToInt32(Console.ReadLine());

            if (escolha == 2)
            {
                i = 0;
            }
            else
            {

            }
        }
    }
    static void exercicio2()
    {
        int i = 1;
        while (i > 0)
        {

            Console.WriteLine("qual o exercicio");
            Console.WriteLine("1 - Calcula o produto e a adição de frações ");
            Console.WriteLine("2 - Calcula o salário de um vendedor");
            Console.WriteLine("3 - Calcula o volume de uma lata de oleo");
            Console.WriteLine("4 - Média harmônica");
            Console.WriteLine("5 - Média ponderada");
            Console.WriteLine("6 - Lê números e calcula operações");
            Console.WriteLine("7 - Calcula potência de número");
            Console.WriteLine("8 - Quadrado de um número");
            Console.WriteLine("9 - Área de um quadrado");
            int escolha1 = Convert.ToInt32(Console.ReadLine());
            switch (escolha1)
            {
                case 1:
                    ProdutoeAdicaoFracao();
                    break;

                case 2:
                    salario();
                    break;

                case 3:
                    VolumeLata();
                    break;

                case 4:
                    MediaHarmonica();
                    break;

                case 5:
                    MediaPonderada();
                    break;

                case 6:
                    LerNumeros();
                    break;

                case 7:
                    Potencia();
                    break;

                case 8:
                    AreaQuadrado();
                    break;

                case 9:
                    NumeroAoQuadrado();
                    break;
            }

            //produto
            static void ProdutoeAdicaoFracao()
            {
                Console.WriteLine("qual o numerador?");
                double num1 = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("qual o denominador?");
                double num2 = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("qual o outro numerador?");
                double num3 = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("qual o outro denominador?");
                double num4 = Convert.ToDouble(Console.ReadLine());
                double formula = num1 / num2;
                double formula2 = num3 / num4;
                Console.WriteLine($"A soma das fracões é {formula + formula2} O produto é {formula * formula2}");
            }
            //produto

            //salario
            static void salario()
            {

                Console.WriteLine("Qual a comissão?");
                double comissao = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("Qual o salario?");
                double salario = Convert.ToDouble(Console.ReadLine());
                double formula = salario * comissao + salario;
                Console.WriteLine($"O salário total é R$ {formula}");
            }
            //salario

            //volumelata
            static void VolumeLata()
            {
                double formula, altura, raio;

                Console.WriteLine("qual o raio da lata de oleo:");
                raio = Convert.ToDouble(Console.ReadLine());

                Console.WriteLine("qual a altura da lata de oleo:");
                altura = Convert.ToDouble(Console.ReadLine());

                formula = Math.PI * Math.Pow(raio, 2) * altura;

                Console.WriteLine($"o volume é {formula}");
            }
            //volumelata

            //mediaHarmonica
            static void MediaHarmonica()
            {
                Console.WriteLine("qual a primeira nota");
                double nota1 = Convert.ToDouble(Console.ReadLine());

                Console.WriteLine("qual a segunda nota");
                double nota2 = Convert.ToDouble(Console.ReadLine());

                Console.WriteLine("qual a terceira nota");
                double nota3 = Convert.ToDouble(Console.ReadLine());

                double n = 3;

                double formula = n / ((1 / nota1) + (1 / nota2) + (1 / nota3));
                Console.WriteLine($" a media ponderada é {formula}");
            }
            //mediaHarmonica


            //mediaPonderada
            static void MediaPonderada()
            {
                Console.WriteLine("qual a primeira nota");
                int nota1 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual o peso da primeira nota");
                int peso1 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual a segunda nota");
                int nota2 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual o peso segunda nota");
                int peso2 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual a terceira nota");
                int nota3 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual o peso terceira nota");
                int peso3 = Convert.ToInt32(Console.ReadLine());

                int formula = ((nota1 * peso1) + (nota2 * peso2) + (nota3 * peso3)) / (peso1 + peso2 + peso3);
                Console.WriteLine($" a media ponderada é {formula}");
            }
            //mediaPonderada

            //ler
            static void LerNumeros()
            {
                Console.WriteLine("qual o primeiro valor");
                int num1 = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("qual o segundo valor");
                int num2 = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine($"A soma dos valores é:{num1 + num2}");
                Console.WriteLine($"A subtração é:{num1 - num2}");
                Console.WriteLine($"A multiplicação dos valores é:{num1 * num2}");
                Console.WriteLine($"A divisão dos valores é:{num1 / num2}");
            }
            //ler

            //Potencia
            static void Potencia()
            {
                Console.WriteLine("Digite o numero");
                int num1 = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("digite o expoente");
                int expoente = Convert.ToInt32(Console.ReadLine());

                double resultado = Math.Pow(num1, expoente);
                Console.WriteLine($"a potencia é {resultado}");
            }
            //Potencia

            //area quadrado
            static void AreaQuadrado()
            {
                Console.WriteLine("Qual o comprimento do lado do quadrado?");
                double lado = Convert.ToDouble(Console.ReadLine());
                double area = Math.Pow(lado, 2);
                Console.WriteLine($"A área do quadrado é {area}");
            }
            //area quadrado

            //Numero Ao quadrado
            static void NumeroAoQuadrado()
            {
                Console.WriteLine("qual o numero?");
                double num = Convert.ToDouble(Console.ReadLine());
                double formula = num * num;
                Console.WriteLine($" a area e {formula}");
            }
            //Numero Ao quadrado

            Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
            int escolha = Convert.ToInt32(Console.ReadLine());

            if (escolha == 2)
            {
                i = 0;
            }
            else
            {

            }
        }
    }


    static void exercicio3()
    {
        int i = 1;
        while (i > 0)
        {

            Console.WriteLine("qual o exercicio");
            Console.WriteLine("1 - Consumo de combustível");
            Console.WriteLine("2 - Troca de valores entre números");
            Console.WriteLine("3 - Entrada e saída de dados");
            Console.WriteLine("4 - Conversão de temperaturas");
            Console.WriteLine("5 - Prestação em atrazo");
            Console.WriteLine("6 - Soma do quadrado de números");
            Console.WriteLine("7 - Salário de funcionário");
            Console.WriteLine("8 - Salário mensal");
            Console.WriteLine("9 - Velocidade de veículo");
            int escolha1 = Convert.ToInt32(Console.ReadLine());
            switch (escolha1)
            {
                case 1:
                    ConsumoCombustível();
                    break;

                case 2:
                    TrocaValor();
                    break;

                case 3:
                    AreaTriangulo();
                    break;

                case 4:
                    Temperatura();
                    break;

                case 5:
                    Prestação();
                    break;

                case 6:
                    SomaQuadrado();
                    break;

                case 7:
                    Funcionário();
                    break;

                case 8:
                    Mensal();
                    break;

                case 9:
                    Velocidade();
                    break;
            }

            //ConsumoCombustível
            static void ConsumoCombustível()
            {
                Console.WriteLine("Quantos quilometros você rodou?");
                int Km = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("Qual o consumo medio de combustivel?");
                int lKm = Convert.ToInt32(Console.ReadLine());

                double formula = Km / lKm;

                Console.WriteLine($"a quantidade de litros consumida é de :{formula}L");
            }
            //ConsumoCombustível

            //TrocaValor
            static void TrocaValor()
            {
                Console.WriteLine("Digite o primeiro número:");
                int num1 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("Digite o segundo número:");
                int num2 = Convert.ToInt32(Console.ReadLine());

                // Trocando valores
                int temp = num1;
                num1 = num2;
                num2 = temp;

                Console.WriteLine($"Após a troca: num1 = {num1}, num2 = {num2}");
            }
            //TrocaValor

            //AreaTriangulo
            static void AreaTriangulo()
            {
                Console.WriteLine("qual a altura do triangulo");
                double altura = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("qual a base do triangulo");
                double bases = Convert.ToDouble(Console.ReadLine());
                double formula = (bases * altura) / 2;
                Console.WriteLine($" a area e {formula}");
            }
            //AreaTriangulo

            //temperatura
            static void Temperatura()
            {
                float temperatura, c, f;
                char unidade;

                Console.WriteLine("qual a temperatura?");
                temperatura = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("qual a unidade de medida?");
                unidade = Char.ToUpper(Convert.ToChar(Console.ReadLine()));
                f = (9 * temperatura) / 5 + 32;
                c = (5 * (temperatura - 32)) / 9;
                if (unidade == 'C' || unidade == 'c')
                {
                    Console.WriteLine($" a temperatura é {f}");
                }

                if (unidade == 'F' || unidade == 'f')
                {
                    Console.WriteLine($" a temperatura é {c}");
                }

            }
            //temperatura

            //Prestação
            static void Prestação()
            {
                Console.WriteLine("Qual o valor do prestação?");
                int aluguel = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("Qual o juros?");
                double juros = Convert.ToDouble(Console.ReadLine());
                double formula = aluguel + juros;
                Console.WriteLine($"o valor total é: {formula}");
            }
            //Prestação

            //SomaQuadrado
            static void SomaQuadrado()
            {
                Console.WriteLine("qual o valor do primeiro numero");
                int num1 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual o valor do segundo numero");
                int num2 = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("qual o valor do terceiro numero");
                int num3 = Convert.ToInt32(Console.ReadLine());

                int formula = (num1 * num1) + (num2 * num2) + (num3 * num3);

                Console.WriteLine($"a soma dos quadrados é: {formula}");
            }
            //SomaQuadrado

            //Funcionário
            static void Funcionário()
            {
                Console.WriteLine("Qual o salario?");
                int salario = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("Qual os valores adicionais?");
                double adicionais = Convert.ToDouble(Console.ReadLine());
                double formula = salario * adicionais + salario;
                Console.WriteLine($"O salario é {formula}");
            }
            //Funcionário

            //Mensal
            static void Mensal()
            {
                Console.WriteLine("Qual a comissão?");
                double comissao = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("Qual o salario?");
                int salario = Convert.ToInt32(Console.ReadLine());
                double formula = salario * comissao + salario;
                Console.WriteLine($"O salario é {formula}");
            }
            //Mensal

            // Velocidade
            static void Velocidade()
            {
                Console.WriteLine("Qual a distancia percorrida?");
                double deltaS = Convert.ToDouble(Console.ReadLine());

                Console.WriteLine("Em quanto tempo?Em horas");
                double deltaT = Convert.ToDouble(Console.ReadLine());

                double formula = deltaS / deltaT;

                Console.WriteLine($"A velocidade do carro é de {formula}Km/h");
            }
            // Velocidade

            Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
            int escolha = Convert.ToInt32(Console.ReadLine());

            if (escolha == 2)
            {
                i = 0;
            }
            else
            {

            }
        }
        Console.WriteLine("quer mudar de exercicio? Aperte 2 para sair");
        int escolha2 = Convert.ToInt32(Console.ReadLine());

        if (escolha2 == 2)
        {
            i = 0;

        }
        else
        {

        }
    }

